{
  "name": "smcoco_com",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/oorococo/smcoco_com.git",
  "license": "MIT",
  "scripts": {
    "dev:web": "bnr dev:web",
    "dev:app": "bnr dev:app",
    "start": "npm run dev:web & npm run dev:app",
    "build:app": "bnr build:app",
    "build:web": "bnr build:web",
    "build": "npm run build:web & npm run build:app",
    "debug": "bnr debug",
    "stop": "bnr stop",
    "prod": "bnr prod"
  },
  "betterScripts": {
    "dev:web": {
      "command": "webpack-dev-server --config .bin/webpack/webpack.config.dev.js -d --hot --inline",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "dev:app": {
      "command": "nodemon .bin/develop.js -w app",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:app": {
      "command": "webpack --config .bin/webpack/webpack.config.server.js -p",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:web": {
      "command": "webpack --config .bin/webpack/webpack.config.prod.js -p",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "debug": {
      "command": "pm2 stop all && pm2 start build/main.js",
      "env": {
        "NODE_ENV": "debug"
      }
    },
    "stop": {
      "command": "pm2 stop all",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "prod": {
      "command": "pm2 stop all && pm2 start build/main.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "ali-oss": "^4.6.3",
    "autoprefixer": "^6.6.1",
    "axios": "^0.15.3",
    "compress": "^0.99.0",
    "compression": "^1.6.2",
    "cors": "^2.8.1",
    "debug": "^2.6.0",
    "express": "^4.14.0",
    "flow": "^0.2.3",
    "font-awesome": "^4.7.0",
    "html-minify": "^0.3.2",
    "immutable": "^3.8.1",
    "log": "^1.4.0",
    "normalize.css": "^5.0.0",
    "pm2": "^2.2.3",
    "react": "^15.4.1",
    "react-addons-css-transition-group": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-hot-loader": "3.0.0-beta.6",
    "react-redux": "^5.0.1",
    "react-router": "4.0.0-alpha.6",
    "redux": "^3.6.0",
    "redux-actions": "^1.2.0",
    "redux-thunk": "^2.1.0",
    "vhost": "^3.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-watch": "^2.0.5",
    "better-npm-run": "^0.0.13",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-compat": "^0.1.1",
    "eslint-plugin-flowtype": "^2.29.2",
    "eslint-plugin-flowtype-errors": "^2.0.3",
    "eslint-plugin-import": "2.1.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.6.0",
    "extract-loader": "^0.1.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "flow-bin": "^0.37.4",
    "html-loader": "^0.4.4",
    "js-beautify": "^1.6.7",
    "node-sass": "^4.3.0",
    "nodemon": "^1.11.0",
    "postcss-loader": "^1.2.2",
    "raw-loader": "^0.5.1",
    "sass-lint": "^1.10.2",
    "sass-loader": "^4.1.1",
    "scss-lint": "^0.0.0",
    "string-replace-loader": "^1.0.5",
    "style-loader": "^0.13.1",
    "stylelint": "asdf",
    "url-loader": "^0.5.7",
    "vorpal": "^1.11.4",
    "webpack": "2.2.0-rc.3",
    "webpack-dev-server": "2.2.0-rc.0",
    "yargs": "^6.6.0"
  }
}
